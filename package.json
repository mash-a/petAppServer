{
  "name": "dogwalkingserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build-client && node ../dogWalkingServer/bin/www",
    "build-client": "cd ../../dogwalkingclient && npm run build",
    "dev": "npm run serve-client & nodemon ../dogWalkingServer/dogWalkingServer/bin/www",
    "postinstall": "cd ../../dogwalkingclient && npm run",
    "serve-client": "cd ../../dogwalkingclient && npm run start",
    "heroku-postbuild": "npm install --production"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "knex": "^0.15.1",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.2",
    "pg": "^7.4.3",
    "yelp-fusion": "^2.0.3"
  }
}
